{
  "name": "@deskpro-apps/toggl",
  "title": "Toggl",
  "description": "Streamline your time management with Toggl",
  "version": "0.0.0",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "targets": [
    {
      "target": "ticket_sidebar",
      "entrypoint": "index.html"
    }
  ],
  "proxy": {
    "whitelist": [
      {
        "url": "https://api.track.toggl.com/api/v9/.*",
        "methods": [
          "GET",
          "POST",
          "PATCH"
        ],
        "timeout": 20
      }
    ]
  }
}